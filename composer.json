{
    "name": "mostlyserious/craft-markerio",
    "description": "Easily add Marker.io to your CraftCMS site.",
    "type": "craft-plugin",
    "license": "proprietary",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^5.0.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "keywords": [
        "craft",
        "craftcms",
        "craft-plugin",
        "marker",
        "marker.io"
    ],
    "support": {
        "email": "devops@mostlyserious.io",
        "issues": "https://github.com/mostlyserious/craft-markerio/issues",
        "source": "https://github.com/mostlyserious/craft-markerio",
        "docs": "https://github.com/mostlyserious/craft-markerio"
    },
    "authors": [
        {
            "name": "Mostly Serious",
            "homepage": "https://github.com/mostlyserious"
        }
    ],
    "autoload": {
        "psr-4": {
            "mostlyserious\\craftmarkerio\\": "src/"
        }
    },
    "extra": {
        "handle": "markerio",
        "name": "Marker.io",
        "developer": "Mostly Serious",
        "documentationUrl": "https://github.com/mostlyserious/craft-markerio"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

